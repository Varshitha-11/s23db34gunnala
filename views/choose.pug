extends layout

block content
  h1= title
  
  - var image_names=["download1.jpg","download2.jpg","download3.jpg","download4.jpg","download5.jpg"]
  - for(let i=0;i<3;i++){
     - let randomNumber=Math.floor(Math.random()*5)
     img(src=`/stylesheets/images/${image_names[randomNumber]}` alt=`${randomNumber}` height="300px" width="300px") 
  - }